IN=${2}
case "$(ps -p $$  | awk 'NR>1 {print $4}' | sed 's/\(.*\)\///')" in
        sh)
                UC=$(echo $INs | tr '[:lower:]' '[:upper:]')
                ;;
        bash)
                UC=${IN^^}
                ;;
        zsh)
                UC=${IN:u}
                ;;
esac
echo "$UC"
